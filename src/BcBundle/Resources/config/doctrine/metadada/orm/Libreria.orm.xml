<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Libreria" table="libreria">
    <id name="idLibreria" type="integer" column="id_libreria">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nif" type="string" column="nif" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tlf" type="string" column="tlf" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="poblacion" type="string" column="poblacion" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="provincia" type="string" column="provincia" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ubicacion" type="string" column="ubicacion" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="web" type="string" column="web" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
